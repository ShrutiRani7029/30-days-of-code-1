///check pallindrome
#include<bits/stdc++.h>
using namespace std;
int main()
{
    int t;
    cin>>t;
    while(t--)
    {
    string s;
    cin>>s;
    int count=0;
    int n=s.length();
    for(int i=0;i<n;i++)
    {
        if(s[i]==s[n-(i+1)])
        count++;
        cout<<s[i]<<" "<<s[n-(i+1)]<<endl;
    }
    if(count==n)cout<<"yes"<<endl;
    else
    cout<<"no"<<endl;
    }
    
}
